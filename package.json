{
  "name": "n8n-nodes-evm-katana",
  "version": "0.1.4",
  "description": "n8n nodes for EVM reads with Katana defaults",
  "keywords": [
    "n8n-community-node-package",
    "evm",
    "katana"
  ],
  "license": "MIT",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && node -e \"require('fs').copyFileSync('package.json','dist/package.json')\" && npm prune --omit=dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/tests/unit/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts'",
    "test:e2e": "jest --testMatch='**/tests/e2e/**/*.test.ts'",
    "test:perf": "jest --testMatch='**/tests/performance/**/*.test.ts'",
    "test:security": "jest --testMatch='**/tests/security/**/*.test.ts'",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "node-fetch": "^3.0.0",
    "viem": "^2.17.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.80.0",
    "n8n-core": "^1.80.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EvmRpcApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EvmContractRead/EvmContractRead.node.js",
      "dist/nodes/EvmTokenBalance/EvmTokenBalance.node.js",
      "dist/nodes/EvmTokenHelper/EvmTokenHelper.node.js",
      "dist/nodes/EvmMulticall/EvmMulticall.node.js"
    ]
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0"
  }
}
